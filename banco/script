CREATE TABLE `Cliente` (
	`id`	INTEGER,
	`nome`	TEXT,
	`sobrenome`	TEXT,
	`email`	TEXT,
	`senha`	TEXT,
	PRIMARY KEY(id)
);
CREATE TABLE `Evento` (
	`id`	INTEGER,
	`nome`	TEXT,
	`data`	TEXT,
	`preco`	REAL,
	`likes`	INTEGER,
	`deslike`	INTEGER,
	`rua`	TEXT,
	`numero`	INTEGER,
	`bairro`	TEXT,
	`cidade`	TEXT,
	`estado`	TEXT,
	`pais`	TEXT,
	`latitude`	TEXT,
	`longitude`	TEXT
);
